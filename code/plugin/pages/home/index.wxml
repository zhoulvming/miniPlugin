<!-- 
<navigator url="./list/index?test=1" hover-class="navigator-hover">点击跳转到商品列表</navigator> -->


<view class="main">
  <swiper indicator-dots="{{indicatorDots}}" 
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" width="100%" height="100%"/>
      </swiper-item>
    </block>
  </swiper>


  <view class="cardlist">
    <card-item></card-item>
  </view>
</view>




